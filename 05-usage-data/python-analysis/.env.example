# FullStory API Configuration
# Copy this file to .env and fill in your values

# Your FullStory API Key (required)
# Get this from: FullStory Settings > API Keys
FULLSTORY_API_KEY=your_api_key_here

# FullStory API Base URL (optional, default shown)
FULLSTORY_API_URL=https://api.fullstory.com

# Default segment ID for exports (optional)
FULLSTORY_DEFAULT_SEGMENT_ID=

# Export directory path (optional, default: ../exports)
EXPORT_DIR=../exports
